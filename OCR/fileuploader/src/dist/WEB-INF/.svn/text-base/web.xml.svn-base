<?xml version="1.0" ?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
    <display-name> Intel TMS Translation File Uploader </display-name>
    
	<servlet>
	  <servlet-name>tmsuploader</servlet-name>
	  <jsp-file>/tmsuploader.jsp</jsp-file>
	  <init-param>
	    <param-name>tmsUploaderHome</param-name>
	    <param-value>tmsuploader</param-value>
	  </init-param>
	</servlet>
	<servlet-mapping>
	  <servlet-name>tmsuploader</servlet-name>
	  <url-pattern>/uploader</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
 		<welcome-file> uploader </welcome-file>
	</welcome-file-list>
</web-app>

